{
  "name": "anytaxtest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "serve": "http-server -p 8080",
    "test:transfer": "cypress run --spec 'cypress/e2e/transfer.cy.js'",
    "test:saucedemo": "cypress run --spec 'cypress/e2e/saucedemo.cy.js'",
    "test:all": "cypress run",
    "test:report": "cypress run --spec 'cypress/e2e/transfer.cy.js' && npm run merge:reports && npm run generate:report",
    "test:report:saucedemo": "cypress run --spec 'cypress/e2e/saucedemo.cy.js' && npm run merge:reports && npm run generate:report",
    "test:report:all": "cypress run && npm run merge:reports && npm run generate:report",
    "merge:reports": "mochawesome-merge cypress/reports/*.json -o cypress/reports/combined-report.json",
    "generate:report": "marge cypress/reports/combined-report.json -f report -o cypress/reports/html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "cypress": "^15.8.1",
    "http-server": "^14.1.1",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.3.2"
  }
}
